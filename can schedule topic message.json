        },
        "network": "testnet",
        "mirrorNetwork": "testnet"
    },
    "test": [
        {
            "request": {
                "type": "AccoutnCreateTransaction",
                "data": {
                    "key": "$this.input.keylist",
                    "iniital": "100 ‚Ñè"
                }
            }
        },
        {
            "name": "topicCreateTransaction",
            "request": {
                "type": "TopicCreateTransaction",
                "data": {
                    "adminKey": "$this.client.operatorKey",
                    "autoRenewAccountId": "$this.client.operatorId",
                    "topicMemo": "HCS Topic_",
                    "submitKey": "$this.input.key2"
                }
            }
        },
        {
            "request": {
                "type": "ScheduleCreateTransaction",
