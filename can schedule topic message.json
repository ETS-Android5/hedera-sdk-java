{
    "input": [
        {
            "key": "key1",
            "value": "$this.generate.key"
        },
        {
            "key": "key2",
            "value": "$this.generate.key"
        },
        {
            "key": "key3",
            "value": "$this.generate.key"
        },
        {
            "key": "keylist",
            "value": [
                "$this.input.key1",
                "$this.input.key2",
                "$this.input.key3"
            ]
        }
    ],
    "client": {
        "operator": {
            "accountId": "0.0.47439",
